  "description": "Unit tests for a sample application",
  "main": "dist/index.js",
  "scripts": {
    "eq": "ts-node ./src/cli/quadraticEquation.cli.ts $0",
    "start": "node ./dist/index.js",
    "start-dev-watch": "ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "start-dev": "ts-node ./src/index.ts",
    "build:ci": "npm build && npm run lint && npm run sequelize:migrate && npm run sequelize:seed && npm run test",
    "build": "npm run lint && npm run build:clean && npm run build:lib",
    "build:clean": "rimraf dist",
    "build:lib": "tsc --declaration",
    "test": "nyc --nycrc-path .nyrc_unit npm run test:unit",
    "test:unit": "npm run test:cmd -- \"tests/unit/**/*.test.ts\"",
    "test:cmd": "mocha --require source-map-support/register --require ts-node/register --full-trace --exit --timeout 50000",
    "lint": "tslint -p tsconfig-lint.json \\\"src/**/*.ts\\\"  \\\"tests/**/*.ts\\\""
  },
